<h1>Paikat</h1>

<table>
  <tr>
    <th>Pituus</th>
    <th>Leveys</th>
    <th>Syvyys</th>
    <th>On olemassa</th>
    <th>Numero</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @docks.each do |dock| %>
  <tr>
    <td><%= dock.length %></td>
    <td><%= dock.width %></td>
    <td><%= dock.depth %></td>
    <td><%= dock.exists %></td>
    <td><%= dock.number %></td>
    <td><%= link_to 'Näytä', dock %></td>
    <td><%= link_to 'Muokkaa', edit_dock_path(dock) %></td>
    <td><%= link_to 'Poista', dock, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

