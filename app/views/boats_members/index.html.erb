<h1>Listing boats_members</h1>
<table>
  <tr>
    <th>Boat</th>
    <th>Member</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @boats_members.each do |boats_member| %>
    <tr>
    <td><%= boats_member.boat.RekNro %></td>
    <td><%= boats_member.member.Nimi %></td>
    <td><%= link_to 'Show', boats_member %></td>
    <td><%= link_to 'Edit', edit_boats_member_path(boats_member) %></td>
    <td><%= link_to 'Destroy', boats_member_path(boats_member), method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

