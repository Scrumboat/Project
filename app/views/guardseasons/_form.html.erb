<%= simple_form_for(@guardseason) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nimi %>
    <%= f.input :guardturn_id, :label => "Guardturn id" %>
    <%= f.input :alku_pvm %>
    <%= f.input :loppu_pvm %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
