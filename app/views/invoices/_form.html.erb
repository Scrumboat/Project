<script type="text/html" id="member_data_in_json">
   <%= @taulu %>
</script>

<%= simple_form_for(@invoice) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nimi %>
    <%= f.input :jno, input_html: { value: params[:jno]}, :input_html => {:onchange => "generateViite()"} %>
    <%= f.input :erapvm, input_html: { value: Time.now + 2.week } %>
    <%= f.input :toimihlokerroin %>
    <%= f.input :talkookerroin %>
    <%= f.input :viitesuoritukset %>
    <%= f.input :suorituksetIlmanViitetta %>
    <%= f.input :suorituksetKassaan %>
    <%= f.input :annetutHyvitykset %>
    <%= f.input :maksetunSummanPalautus %>
    <%= f.input :liittymismaksu %>
    <%= f.input :jasenmaksu %>
    <%= f.input :edellisenKaudenLaskutus %>
    <%= f.input :venerekisterimaksu %>
    <%= f.input :varastokoppimaksu %>
    <%= f.input :laiturimaksu %>
    <%= f.input :ensirekisterimaksu %>
    <%= f.input :telakkamaksu %>
    <%= f.input :muutMaksut %>
    <%= f.input :talkoosakko %>
    <%= f.input :katsastussakko %>
    <%= f.input :vartiosakko %>
    <%= f.input :laskutuslisa %>
    <%= f.input :luontipvm %>
    <%= f.input :lahetyspvm %>
    <%= f.input :tunniste %>
    <%= f.input :maksettu %>
    <% #@laskut.each do |lasku| %>
    <% #lasku.viitenumero %>
    <% #end %>
    <%= @taulu %>
    <% #@numero %>
    <%= f.hidden_field :viitenumero, :value => "11" %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
